{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "Redis": "redis:6379",
    "Default": "Host=postgres;Database=gateway;Username=nav;Password=nav"
  },
  "RabbitMq": {
    "Host": "rabbitmq",
    "Username": "guest",
    "Password": "guest"
  },
  "KeycloakAdmin": {
    "BaseUrl": "http://navigation-keycloak:8080",
    "Realm": "navigation",
    "AdminRealm": "master",
    "ClientId": "admin-cli"
  },
  "Auth": {
    "AuthorityPublic": "http://localhost:8080/realms/navigation",
    "AuthorityInternal": "http://navigation-keycloak:8080/realms/navigation",
    "Audience": "navigation-api",
    "ClientId": "navigation-api",
    "RedirectUri": "http://localhost:5000/api/auth/callback",
    "SpaBaseUrl": "http://localhost:5173"
  },
  "ReverseProxy": {
    "Routes": {
      "journeys_api": {
        "ClusterId": "journeys",
        "Match": {
          "Path": "/api/{**catch-all}"
        }
      },
      "journeys_public": {
        "ClusterId": "journeys",
        "Match": {
          "Path": "/public/{**catch-all}"
        }
      },
      "admin_journeys": {
        "ClusterId": "journeys",
        "AuthorizationPolicy": "Admin",
        "Match": {
          "Path": "/admin/journeys/{**catch-all}"
        }
      },
      "admin_statistics": {
        "ClusterId": "journeys",
        "AuthorizationPolicy": "Admin",
        "Match": {
          "Path": "/admin/statistics/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "journeys": {
        "Destinations": {
          "d1": {
            "Address": "http://journey-service:8080/"
          }
        }
      }
    }
  },
  "AllowedHosts": "*"
}
